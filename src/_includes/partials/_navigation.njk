<header class="bg-black text-gray-50 py-4">
<nav class="max-w-2xl mx-auto text-lg space-x-6">
	{% for item in collections.page %}
  {% if not item.data.pagination %}
    <a href="{{ item.url }}"
       {% if item.url == page.url %}aria-current='page' data-state='active'{% endif %}
       class="hover:text-sky-400">
       {{ item.data.title }}
    </a>
  {% endif %}
{% endfor %}
</nav>

</header>